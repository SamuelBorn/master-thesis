      \strng{namehash}{af21695f56b470201ddddda5a92b2521}
      \strng{fullhash}{af21695f56b470201ddddda5a92b2521}
      \strng{fullhashraw}{af21695f56b470201ddddda5a92b2521}
      \strng{bibnamehash}{af21695f56b470201ddddda5a92b2521}
