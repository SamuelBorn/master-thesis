@inproceedings{sanders_think_2013,
    location = {Berlin, Heidelberg},
    title = {Think Locally, Act Globally: Highly Balanced Graph Partitioning},
    isbn = {978-3-642-38527-8},
    doi = {10.1007/978-3-642-38527-8_16},
    shorttitle = {Think Locally, Act Globally},
    abstract = {We present a novel local improvement scheme for graph partitions
                that allows to enforce strict balance constraints. Using negative
                cycle detection algorithms this scheme combines local searches
                that individually violate the balance constraint into a more
                global feasible improvement. We combine this technique with an
                algorithm to balance unbalanced solutions and integrate it into a
                parallel multi-level evolutionary algorithm, {KaFFPaE}, to tackle
                the problem. Overall, we obtain a system that is fast on the one
                hand and on the other hand is able to improve or reproduce many
                of the best known perfectly balanced partitioning results
                reported in the Walshaw benchmark.},
